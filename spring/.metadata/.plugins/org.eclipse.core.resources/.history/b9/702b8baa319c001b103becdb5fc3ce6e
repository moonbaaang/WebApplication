package emp;

public class EmpMain {

	public static void main(String[] args) {
		EmpVO vo = new EmpVO();
		vo.setId(100);
		vo.setName("이사원");
		vo.setSalary(12302410.10);
		EmpDAO dao = new EmpDAO();
		dao.insertEmp(vo); //주입
		
		EmpVO vo2 = new EmpVO();
		vo.setId(100);
		vo.setName("이사원");
		vo.setSalary(12302410.10);
		EmpDAO dao = new EmpDAO();
		dao.insertEmp(vo2); //주입
		
		System.out.println("회원등록을 마쳤습니다.");
	}

}
