<script type="text/javascript">

var b = "<br>";
var ar1 = [1, "java", true, 200.99, "300"];
ar1.push(100);
ar1.unshift(200);
ar1.pop();
ar1.shift();


document.write(ar1.length + b);
document.write(ar1+b);
document.write(ar1.join("/")+b);
for(var i =0 ; i<ar1.length; i++){
	document.write(i+":"+ar1[i]+b);
}
for(var i in ar1){
	document.write(i+":"+ar1[i]+b);
}


var ar2 = new Array();
ar.push(100);
ar.unshift(200);
document.write(ar2.join("|"));

</script>
